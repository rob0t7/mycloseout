<script  type="text/javascript" language="javascript">

    var visible = true;
    var nextNode;
    var afterNode;
    var descNode;
    var theNode;
    var childNode;
    var isIE;
    var theTd;
    var paraChildren;
    var imgChildren;
    var spanChildren;  
    var callHideDescript = hideDescriptions();

    
    window.onload = function()
    {
        eval(callHideDescript);
    }

    function checkBrowser()
    {
        if (document.all)
        {
            isIE = true;
        }
        else
        {
            isIE = false;
        }
    }

    function checkIEDescription()
    {
        for (var i = 0; i < paraChildren.length; i++)
        {
            
            if (paraChildren[i].innerHTML.length > 0)
            {
                paraChildren[i].hasDescription = true;
            }
            else
            {
                paraChildren[i].hasDescription = false;
            }
        }
    }

    function checkDescription()
    {
        for (var i = 0; i < paraChildren.length; i++)
        {
            if (paraChildren[i].innerHTML.length > 56)
            {
                paraChildren[i].hasDescription = true;
            }
            else
            {
                paraChildren[i].hasDescription = false;
            }

        }

    }

    function getElements()
    {
        theTd = document.getElementById('openHouseTD');

        paraChildren = theTd.getElementsByTagName("P");

        imgChildren = theTd.getElementsByTagName("img");
      
        spanChildren = theTd.getElementsByTagName("span");

    }
    
    function hideDescriptions()
    {
        if (document.getElementById('openHouseTD'))
        {
            getElements();
        
            // determine browser type
            checkBrowser();

            if (isIE)
            {
                checkIEDescription();
            }
            else
            {
                checkDescription();
            }

            // hide all the descriptions
            for (var i = 0; i < paraChildren.length; i++)
            {
                // if the node has description, hide it       
                if (paraChildren[i].hasDescription)
                {                    
                    paraChildren[i].style.display = "none";
                    imgChildren[i].src = "presentation/images/en-CA/icons/but_plus.gif";
                }
                else //if not, hide the image
                {
                    imgChildren[i].style.visibility = "hidden";
                }
            }
            setFirstNode(); // expand the first node

        }
        else
        {
            return;

        }
           

    }


    function setFirstNode()
    {
        paraChildren[0].style.display = "block";
        imgChildren[0].src = "presentation/images/en-CA/icons/but_minus.gif";
        spanChildren[0].expanded = true;
    }

    function getNextSibling(theNode)
    {
        // gets the div in ie but something else in others
        nextNode = theNode.nextSibling;

        //if the next node is not an element (used for firefox and safari)
        if (nextNode.nodeType != 1)
        {
            // if it is not a real element like div or span get next one till a real one is grabbed
            while (nextNode.nodeType != 1)
            {
                nextNode = nextNode.nextSibling;

            }

            // trying to get the next div
            afterNode = nextNode.nextSibling;

            if (afterNode.nodeType != 1)
            {
                // if it is not a real element or a BR the, get the node, thiss gets the div 
                while (afterNode.nodeType != 1 || afterNode.tagName == "BR")
                {
                    afterNode = afterNode.nextSibling;
                }

                descNode = afterNode;
            }
        }
        else
        {
            // used for it, which makes it easier to get the div
            if (nextNode.nextSibling.tagName == "BR")
            {
                descNode = (nextNode.nextSibling).nextSibling;

            }

        }

        // get the paragraph section inside the div
        getPara();
    }

    function getPara()
    {
        // get all the paragraphs in the div and then the first paragraph
        var paraNode = descNode.getElementsByTagName('p');
        descNode = paraNode[0];

    }

    function setImage(element)
    {

        var i = 0;

        // find the image tag
        while (element.childNodes[i].tagName != "IMG")
        {
            i++;
        }

        if (element.expanded)
        {
            // set the expended image for the node and the flag
            element.childNodes[i].src = "presentation/images/en-CA/icons/but_plus.gif";
            element.expanded = false;
        }
        else
        {
            element.childNodes[i].src = "presentation/images/en-CA/icons/but_minus.gif";
            element.expanded = true;
        }
    }

    function expandDesc()
    {
        // set the paragraph to visible
        descNode.style.display = "block";
        setZoomBar();
    }

    function collapseDesc()
    {
        // set the paragraph to hidden
        descNode.style.display = "none";
        setZoomBar();
    }

    function changeView(theElement)
    {
        // find the paragraph that the image has recieved the click event for
        getNextSibling(theElement);

        // if the description is expanded, collapse it and vise versa
        if (theElement.expanded == true)
        {
            collapseDesc();
        }
        else
        {
            expandDesc();
        }
        // set the correct image based on state
        setImage(theElement);
        return;
    }
    
</script>


